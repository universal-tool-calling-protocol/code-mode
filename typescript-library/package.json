{
  "name": "@utcp/code-mode",
  "version": "1.2.11",
  "description": "Code execution mode for UTCP - enables executing TypeScript code chains with tool access using isolated-vm for security",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MPL-2.0",
  "author": "UTCP Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/universal-tool-calling-protocol/code-mode.git",
    "directory": "typescript-library"
  },
  "keywords": [
    "utcp",
    "code",
    "typescript",
    "execution",
    "tool calling",
    "chain",
    "isolated-vm",
    "sandbox"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {},
  "peerDependencies": {
    "@utcp/sdk": "^1.1.0",
    "isolated-vm": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "isolated-vm": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
    "@utcp/sdk": "^1.1.0",
    "@utcp/direct-call": "^1.1.0",
    "isolated-vm": "^6.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
